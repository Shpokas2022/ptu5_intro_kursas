    "import logging\n",
    "logging.basicConfig(level = logging.DEBUG)\n",
    "\n",
    "def dalyba(a, b)\n",
    "try:\n",
    "    res = a / b\n",
    "except Exception as e:\n",
    "    logging.error(str(e)+str(e.__class__))\n",
    "    else:\n",
    "        logging.info(f\"dalyba: {a} / {b} = {res}\")\n",
    "    return res"
   ]
  }
